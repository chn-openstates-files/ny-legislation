  "identifier": "S54",
  "title": "Prohibits the use of intoxication of the victim as a defense in sex crimes",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Prohibits the use of intoxication of the victim as a defense in sex crimes where the victim is under the extreme influence of any substance which renders them incapable of appraising or controlling such victim's conduct and incapable of clearly expressing lack of consent."
    }
  ],
