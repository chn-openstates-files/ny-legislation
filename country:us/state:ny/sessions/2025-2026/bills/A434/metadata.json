{
  "legislative_session": "2025-2026",
  "identifier": "A434",
  "title": "Relates to amending provisions requiring the termination of connected vehicle services upon the request of a domestic violence victim",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends certain provisions requiring the termination of connected vehicle services upon the request of a domestic violence victim; requires the provider of the connected vehicle services to terminate such services within two days of receiving a request; directs the office for the prevention of domestic violence to make information available on their website; defines terms."
    }
  ],
