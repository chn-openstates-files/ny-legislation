  "title": "Relates to food service item waste reduction",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Prohibits public food service establishments operating in the state from providing excess food service items including utensils, condiments, napkins and extra containers, to any customer except upon, and in accordance with, the express request of that customer."
    }
